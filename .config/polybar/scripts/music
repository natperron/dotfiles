echo "%{A1:spotify:}%{T8}%{B#6272a4}%{F#bd93f9}%{F- B-}%{T-}%{T4}%{B#bd93f9}%{F#f8f8f2}%{F- B-}%{T-}%{T8}%{B#c2282a36}%{F#bd93f9}%{B- F-}%{T-}%{A}" ;
